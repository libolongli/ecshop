num	widget\blog_sohu.php	/^$num = !empty($_GET['num']) ? intval($_GET['num']) : 10;$/;"	v
promote_price	widget\blog_sohu.php	/^        $promote_price = bargain_price($row['promote_price'], $row['promote_start_date'], $row['promote_end_date']);$/;"	v
result	widget\blog_sohu.php	/^$result = $db->getAll("SELECT goods_id, goods_name, shop_price, promote_price, promote_start_date, promote_end_date, goods_brief, goods_thumb FROM " . $ecs->table('goods') . " WHERE is_on_sale = 1 AND is_alone_sale = 1 AND is_delete = 0 ORDER BY rand() LIMIT 0, $num");$/;"	v
